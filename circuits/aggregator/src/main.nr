use dep::std;

fn main(
    // their proof
    their_vk : [Field; 114], 
    their_proof : [Field; 96],
    their_key_hash : Field, 
    their_input_aggregation : [Field; 16],

    // what I need to verify their proof
    their_random_number : pub Field,
    my_previous_guess : pub Field,
    their_hashed_number : pub Field,

    // their inputs to their turn
    my_vk : [Field; 114],
    my_proof : [Field; 96],
    my_key_hash : Field,

    my_random_number : pub Field,
    their_guess : pub Field,
    my_hashed_number : pub Field,
) -> pub [Field; 16] {
    
    // so I got this bastard's proof that I failed. I need to verify it.
    // first I need to prepare my public inputs
    let public_inputs_to_their_proof = [
        their_random_number,
        my_previous_guess,
        their_hashed_number
    ];

    // now I can use them to verify their proof
    let their_proof_input_aggregation = std::verify_proof(
        their_vk.as_slice(), 
        their_proof.as_slice(), 
        public_inputs_to_their_proof.as_slice(), 
        their_key_hash, 
        their_input_aggregation
    );

    let public_inputs_to_my_proof = [
        my_random_number, 
        their_guess, 
        my_hashed_number
    ];

    // now it's their turn, I hope they fail!
    // I wrapped my proof in another proof
    // so I'm essentially verifying my own proof here
    std::verify_proof(
        my_vk.as_slice(), 
        my_proof.as_slice(), 
        public_inputs_to_my_proof.as_slice(), 
        my_key_hash, 
        their_proof_input_aggregation
    )
}

